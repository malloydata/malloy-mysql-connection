{
  "name": "@malloydata/malloy-mysql-connection",
  "version": "1.0.0",
  "description": "Provides a connection and dialect to be able to execute Malloy models against MySQL databases.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "workspaces":{
    "packages": [
      "test"
    ]
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest --config=jest.config.js",
    "run": "ts-node src/index.ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "malloy-link": "npm --no-fund --no-audit link @malloydata/malloy @malloydata/malloy-tests",
    "malloy-unlink": "npm --no-fund --no-save --no-audit unlink @malloydata/malloy @malloydata/malloy-tests && npm --no-fund --no-audit install --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malloydata/malloy-mysql-connection.git"
  },
  "keywords": [
    "malloy",
    "mysql",
    "connection"
  ],
  "author": "Malloy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malloydata/malloy-mysql-connection/issues"
  },
  "homepage": "https://github.com/malloydata/malloy-mysql-connection#readme",
  "dependencies": {
    "@malloydata/db-bigquery": "^0.0.62",
    "@malloydata/malloy": "^0.0.62",
    "@types/mysql": "^2.15.21",
    "mysql": "^2.18.1",
    "jest": "^29.6.1",
    "jsdom": "^22.1.0",
    "luxon": "^3.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@types/jest": "^29.2.1",
    "@types/jest-expect-message": "^1.0.3",
    "@types/jsdom": "^21.1.1",
    "@types/luxon": "^3.3.1",
    "jest": "^29.0.3",
    "jest-diff": "^27.0.6",
    "jest-expect-message": "^1.1.3",
    "jest-silent-reporter": "^0.5.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "gts": "^3.1.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0"
  }
}
